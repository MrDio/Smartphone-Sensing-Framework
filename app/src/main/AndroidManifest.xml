<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.example.ssf.mma">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/abc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/abc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name="edu.example.ssf.mma.userInterface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.example.ssf.mma.userInterface.CharacterOverviewActivity" > </activity>
        <activity android:name="edu.example.ssf.mma.userInterface.RecognizeActivity" > </activity>
        <activity android:name="edu.example.ssf.mma.charts.AccChart" > </activity>
        <activity android:name="edu.example.ssf.mma.hardwareAdapter.GPS.gps" > </activity>
    </application>

</manifest>